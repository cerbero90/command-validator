{
    "name": "cerbero/command-validator",
    "type": "library",
    "description": "Validate Laravel console commands input.",
    "keywords": [
        "cerbero",
        "command-validator",
        "console",
        "commands",
        "validator",
        "validation"
    ],
    "homepage": "https://github.com/cerbero90/command-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Marco Sartori",
            "email": "andrea.marco.sartori@gmail.com",
            "homepage": "https://github.com/cerbero90",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0.8",
        "illuminate/contracts": "^5.5|^6.0|^7.0",
        "symfony/console": ">=3.3.7"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0|^5.0",
        "phpunit/phpunit": ">=6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Cerbero\\CommandValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerbero\\CommandValidator\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}